<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome to the SpaceBar{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    {% block stylesheets %}
        <link type="text/css" rel="stylesheet" href="{{ asset('libraries/bootstrap-4.3.1/css/bootstrap.min.css') }}">
        <link type="text/css" rel="stylesheet" href="{{ asset('/libraries/font-awesome-4.7.0/css/font-awesome.min.css') }}">
        <link type="text/css" rel="stylesheet" href="{{ asset('libraries/datatables_1.10.20/css/datatables_bootstrap4_1.10.20.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    {% endblock %}
</head>

<body>
<nav class="navbar navbar-expand-lg navbar-dark navbar-bg mb-5">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavDropdown">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item">
                <a style="color: #fff;" class="nav-link" href="#">Local Asteroids</a>
            </li>
            <li class="nav-item">
                <a style="color: #fff;" class="nav-link" href="#">Weather</a>
            </li>
        </ul>
    </div>
</nav>

{% block body %}{% endblock %}

<footer class="footer">
    <div class="container text-center">
        <span class="text-muted">Made with <i class="fa fa-heart" style="color: red;"></i> by Me <a href=""></a></span>
    </div>
</footer>

{% block javascripts %}
    <script type="text/javascript" src="{{ asset('js/jquery_3.4.1.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/twitter_bootstrap_4.3.1.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('libraries/datatables_1.10.20/js/datatables_1.10.20.min.js') }}"></script>
{% endblock %}
</body>
</html>
